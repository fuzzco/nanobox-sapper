{
  "name": "toba",
  "version": "1.0.0",
  "description": "Toba Capital",
  "main": "index.js",
  "scripts": {
    "init": "./scripts/init.sh",
    "prestart": "./scripts/check-install.sh",
    "build": "rollup -c --environment dev",
    "dev": "parallelshell 'npm run js' 'npm run serve' 'npm run css'",
    "serve": "nanobox run php-server",
    "js": "rollup -c -w",
    "precss": "node-sass src/scss/ -o app/",
    "css": "node-sass src/scss -o app/ --watch",
    "img": "imagemin-newer src/img/ app/img/",
    "prestart": "npm run img",
    "stage": "nanobox deploy toba-staging",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.oef:nikolaswise/toba.git"
  },
  "keywords": [
    "Toba"
  ],
  "author": "Fuzzco",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "clean-css": "^4.1.9",
    "imagemin-newer": "^1.0.2",
    "node-sass": "^4.7.2",
    "parallelshell": "^3.0.2",
    "postcss-cli": "^5.0.0",
    "postcss-import": "^11.1.0",
    "rollup": "^0.56.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-svelte": "^4.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "svelte": "^1.55.0"
  }
}
